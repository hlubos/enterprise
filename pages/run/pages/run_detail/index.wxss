}

.title2-right-id {
  display: flex;
  justify-content: flex-end;
  /* flex-direction: row-reverse; */
  text-align: right;
  width: 100%;
  font-size: 24rpx;
  color: #999999;
}

.title3 {
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  margin-bottom: 48rpx;
}

.title3-item1,
.title3-item2,
.title3-item3,
.title3-item4 {
  width: 50%;
  text-align: center;
  padding-bottom: 50rpx;
}

.title3-num {
  font-weight: Bold;
  font-size: 48rpx;
  color: #333333;
}

.title3-unit {
  font-size: 24rpx;
  color: #666666;
}

.count-line {
  position: relative;
  width: 100%;
  height: 1rpx;
  border-top: 1px rgb(49, 218, 175) solid;
  margin-top: 24rpx;
  display: flex;
  justify-content: center;
  margin-bottom: 30rpx;
}

.count-line-txt {
  position: absolute;
  background-color: #ffffff;
  padding-top: 5rpx;
  /* padding-left: 20rpx; */
  /* padding-right: 20rpx; */
  top: -24rpx;
  font-size: 24rpx;
  width: 300rpx;
  height: 24rpx;
  color: rgb(49, 218, 175);
  text-align: center;
}

.speed-table {
  position: relative;
  width: 654rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}
.speed-line{
  position: absolute;
  left: 50%;
  top:35%;
  transform: translateX(-50%);
  width: 1rpx;
  height: 100rpx;
  border-left: solid 1rpx #ccc;
}

