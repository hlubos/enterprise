}

.title1 {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 16rpx;
}

.logo {
  width: 174rpx;
  height: 46rpx;
}

.logo image {
  width: 100%;
  height: 100%;
}

.finish-time {
  margin-left: 24rpx;
  flex: 1;
  font-size: 24rpx;
  color: #999999;
}

.avatar {
  width: 84rpx;
  height: 84rpx;
  border-radius: 50%;
  background: #d8d8d8;
}

.avatar image {
  width: 100%;
