  width: 50%;
}

.title2-left-kmeter {
  font-weight: bold;
  font-size: 80rpx;
  color: #333333;
  line-height: 80rpx;
}

.title2-left-unit {
  font-size: 28rpx;
  color: #666666;
}

.title2-right {
  display: flex;
  justify-content: end;
  align-items: end;
  flex-wrap: wrap;
  flex: 1;
  /* float: right; */
}

.title2-right-name {
  display: flex;
  justify-content: flex-end;
  /* flex-direction: row-reverse; */
