<view class="page">
    <view class="page__bd">
        <background-video id="backgroundVideo"></background-video>
        <camera binderror="error" class="camera" devicePosition="front" flash="off" frameSize="medium">
            <canvas canvasId="canvas" class="canvas" id="canvas"></canvas>
        </camera>
        <cover-view class="stage">
            <inner-device-angle angleRange="{{angleRange}}" bind:angleSuccess="angleSuccess" bind:stopGame="stopGame" code="{{code}}" gameEnd="{{gameEnd}}" gameStart="{{gameStart}}" id="inner-device-angle" wx:if="{{showDevicePage}}"></inner-device-angle>
            <skin skinCode="{{userInfo.skinCode}}" wx:if="{{userInfo.skinCode}}"></skin>
            <game-column gameDuration="{{time}}" score="{{num&&num-1}}" wx:if="{{gameStart}}"></game-column>
            <cover-view class="gameEnd_box" wx:if="{{gameEnd}}">
                <cover-view class="gameEnd_box_container">
                    <cover-view class="gameEnd_box-hd">
                        <cover-view style="color:#FF6A00;font-size:46px;">
            {{num&&num-1}}
            </cover-view>
                        <cover-view style="color:#333333;font-size:36rpx;margin-top:20px;">
            个数
            </cover-view>
                        <cover-view style="color:#333333;font-size:32rpx;margin-top:20rpx; display:flex;align-items:center;">
                            <cover-view style="font-size:28rpx;text-align:center;">获得积分</cover-view>
                            <cover-view style="color:#EB7D3C;margin-left: 20rpx;min-width: 40rpx;display:inline-block;">
              {{integral}}
              </cover-view>
                        </cover-view>
                    </cover-view>
                    <cover-view style="font-size:30rpx;text-align:center;">
            好厉害！！你已超越了{{rate?rate:''}}%的小伙伴
          </cover-view>
                    <cover-view class="gameEnd_box-ft">
                        <cover-view bindtap="goBack" class="gameEnd_box-ft-button">结束运动</cover-view>
                        <cover-view bindtap="reStart" class="gameEnd_box-ft-button active">重新开始 {{reStartCountDown}}</cover-view>
                    </cover-view>
                </cover-view>
                <cover-image class="result_bg" src="https://go-ran-pic.lovedabai.com/static/resultBackgroundHead.png"></cover-image>
            </cover-view>
            <cover-view class="blue point top_mid {{top_mid?'green':''}}" wx:if="{{!doClose&&!gameEnd||1}}"></cover-view>
            <cover-view class="blue point bot_mid {{bot_mid?'green':''}}" wx:if="{{doClose&&!gameEnd||1}}"></cover-view>
            <cover-view class="blue point left_mid {{left_mid?'green':''}}" style="left:{{left_point_pos}}px" wx:if="{{!doClose&&!gameEnd||1}}"></cover-view>
            <cover-view class="blue point right_mid {{right_mid?'green':''}}" style="left:{{right_point_pos}}px" wx:if="{{!doClose&&!gameEnd||1}}"></cover-view>
            <cover-view class="fetch-box" wx:if="{{!gameEnd&&!gameStart}}"></cover-view>
            <cover-view class="img_tips" wx:if="{{show_tip&&!gameEnd&&!gameStart}}">
                <cover-image class="img_tips_img" src="https://go-ran.oss-cn-shanghai.aliyuncs.com/static/YWQZ_tips.png"></cover-image>
            </cover-view>
            <cover-image class="img_tips_img_pose" src="https://go-ran.oss-cn-shanghai.aliyuncs.com/static/YWQZ_tips_pose.png" wx:if="{{show_tip&&!gameEnd&&!gameStart}}"></cover-image>
            <cover-image class="img_in_box" src="https://go-ran.oss-cn-shanghai.aliyuncs.com/static/backOff-tips.png" wx:if="{{show_in_box}}"></cover-image>
            <start-countdown startCountdown="{{countDownSrc}}" wx:if="{{countDown}}"></start-countdown>
            <cover-image class="img_start" src="https://go-ran.oss-cn-shanghai.aliyuncs.com/static/countdown-start.png" wx:if="{{countDown===0}}"></cover-image>
        </cover-view>
    </view>
</view>
