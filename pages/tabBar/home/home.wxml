<!-- pages/tabBar/home/home.wxml -->
<view class="nav">
    <view class="tle active-tle">
        <view class="name">AI运动</view>
        <view class="tag"></view>
    </view>
    <login-btn bindsuccess="gotoRun">
        <view class="tle">
            <view class="name">跑步</view>
            <view class="tag"></view>
        </view>
    </login-btn>
</view>
<view class="header">
    <view class="left">
        <view class="title">
            <text>AI动作库</text>
        </view>
        <view class="desc">
            <text>Yodo exercises</text>
        </view>
    </view>
    <image class="right" src="https://ssl-pubpic.51yund.com/1040424039.png"></image>
</view>
<login-btn bindsuccess="gotoRecords">
    <view class="records">
        <view class="records-left">历史记录</view>
        <image class="records-right" src="https://ssl-pubpic.51yund.com/1004075363.jpg" />
    </view>
</login-btn>
<view class="kind-list">
    <kind-item class="item" wx:for="{{kindList}}" wx:key="video_id" info="{{item}}"></kind-item>
</view>