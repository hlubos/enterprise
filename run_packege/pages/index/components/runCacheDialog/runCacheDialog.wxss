/* run_packege/pages/index/components/runCacheDialog/runCacheDialog.wxss */
.mask {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100vh;
    background: rgba(0,0,0,0.50);
    display: flex;
    justify-content: center;
    align-items: center;
    z-index: 1000;
}
.dialog-box {
    width: 622rpx;
    height: 504rpx;
    background: #FFFFFF;
    border-radius: 16rpx;
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
}
.dialog-title{
    width: 100%;
    font-size: 36rpx;
    color: #333333;
    text-align: center;
    margin-top: 80rpx;
}
.giveup-btn{
    width: 370rpx;
    height: 80rpx;
    background: #F86254;
    border-radius: 8rpx;
    font-weight: 600;
    font-size: 34rpx;
    color: #FFFFFF;
    margin-bottom: 20rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
.continue-btn{
    width: 370rpx;
    height: 80rpx;
    background: #4CDDB4;
    border-radius: 8rpx;
    font-weight: 600;
    font-size: 34rpx;
    color: #FFFFFF;
    /* margin-top: 20rpx; */
    margin-bottom: 64rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}