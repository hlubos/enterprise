}

.icon-fanzhuanxiangjibeifen3:before {
  content: "\e676";
}

.content {
  height: 100vh;
  background-size: 100% 100%;
}

.content .content_opacity {
  position: relative;
  width: 100%;
  height: 100vh;
  background-color: rgba(255, 255, 255, 0.5);
}

.content .content_opacity .camera {
  width: 100vw;
  height: 100vh;
  z-index: -5;
  position: relative;
}

.content .content_opacity .camera .canvas {
  width: 100%;
  height: 100%;
  z-index: 0;
}

.content .content_opacity .content_remind {
  position: fixed;
  background-color: rgba(51, 55, 68, 0.8);
  text-align: center;
  line-height: 160rpx;
  color: #ffffff;
  font-size: 72rpx;
  height: 160rpx;
  width: 100%;
  top: 0;
  z-index: 10;
}

.content .content_opacity .content_text {
  width: calc(36rpx*2);
  height: calc(260rpx*2);
  border-radius: 260rpx;
  background-size: 100% 100%;
  background-color: rgba(255, 255, 255, 0.5);
  position: absolute;
  left: 45.3%;
  top: 33.3%;
  z-index: 10;
}

.content .content_opacity .content_text .content_top {
  position: absolute;
  left: -12rpx;
}

.content .content_opacity .content_text .content_top .content_top_left {
  position: absolute;
  left: -135rpx;
  top: 16rpx;
  display: inline-block;
  width: calc(40rpx*2);
  height: calc(32rpx*2);
  background: url("https://ssl-pubpic.51yund.com/1040234039.png") no-repeat;
  background-size: 100% 100%;
}

.content .content_opacity .content_text .content_top .content_top_right {
  position: absolute;
  display: inline;
  background-size: 100% 100%;
  width: calc(48rpx*2);
  height: calc(48rpx*2);
}

.content .content_opacity .content_text .content_buttom {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  width: 114rpx;
  height: 114rpx;
}

.content .content_opacity .content_text .content_buttom .content_buttom_circular {
  position: absolute;
  background-color: rgba(255, 255, 255, 0.9);
  background: url("https://ssl-pubpic.51yund.com/1040233265.png") no-repeat;
  background-size: 100% 100%;
  border-radius: 100%;
  left: -21rpx;
  width: 114rpx;
  height: 114rpx;
}

.content .content_opacity .content_text .content_buttom .canvas_time {
  left: -21rpx;
  width: 114rpx;
  height: 114rpx;
  border-radius: 100%;
  text-align: center;
  background-color: #FFFFFF;
  line-height: 114rpx;
  position: absolute;
  z-index: 99;
}

.content .content_opacity .button_buttom {
  position: fixed;
  bottom: 48rpx;
  width: 100%;
  height: 64rpx;
  z-index: 10;
}

.content .content_opacity .button_buttom button {
  padding: 0;
  width: 140rpx;
  height: 64rpx;
  font-size: 28rpx;
  text-align: center;
  line-height: 64rpx;
  border-radius: 32rpx;
  color: #FFFFFF;
  background-color: #333744;
}

.content .content_opacity .button_buttom .buttom_left {
  position: absolute;
  left: 16rpx;
}

.content .content_opacity .button_buttom .buttom_right {
  position: absolute;
  right: 16rpx;
}